load "foo.ledger"
  string-key: "string-value"

option "foo" "bar"
  account-key: Assets:Foo

2025-06-01 open Assets:Bar
  currency-key: CHF

2025-06-01 close Assets:Bar
  date-key: 2025-06-02

2025-06-01 balance
  string-key: "string-value"
  Assets:Foo  0 CHF

2025-06-01 * "Test"
  number-key: 1,234.5
  Assets:Foo  1 CHF
    amount-key:  1

2025-06-01 commodity CHF
  name: "Swiss Franc"

2025-06-01 currency EUR
  name: "Euro"
